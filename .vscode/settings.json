{
  "cSpell.ignorePaths": [
    "coverage",
    "extensions.json",
    "node_modules",
    "settings.json"
  ],
  "cSpell.words": [
    "composables",
    "datetime",
    "Parens",
    "Pinia",
    "tabindex",
    "vite",
    "vitepress",
    "vuejs",
    "vuetify",
    "vueuse",
    "vuex",
    "webfontloader"
  ],
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true
}
